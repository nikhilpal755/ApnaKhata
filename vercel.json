{
    "version":2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://apnakhata.netlify.app"
            }
        }
    ]
}
